sub Charsheet()
######Trope subroutine#####
	local really
	really = alert "Do you really want to insert all the available High-ASCII and Unicode characters into this document?", "Yes", "No", "", "warning"
	if really = 2 return
	restore 1: InsertIntoCharsheet("Ascii", "1")
	restore A: InsertIntoCharsheet("Ascii", "A")
	restore B: InsertIntoCharsheet("Ascii", "B")
	restore C: InsertIntoCharsheet("Ascii", "C")
	restore D: InsertIntoCharsheet("Ascii", "D")
	restore E: InsertIntoCharsheet("Ascii", "E")
	restore F: InsertIntoCharsheet("Ascii", "F")
	restore G: InsertIntoCharsheet("Ascii", "G")
	restore H: InsertIntoCharsheet("Ascii", "H")
	restore I: InsertIntoCharsheet("Ascii", "I")
	restore J: InsertIntoCharsheet("Ascii", "J")
	restore K: InsertIntoCharsheet("Ascii", "K")
	restore L: InsertIntoCharsheet("Ascii", "L")
	restore M: InsertIntoCharsheet("Ascii", "M")
	restore N: InsertIntoCharsheet("Ascii", "N")
	restore O: InsertIntoCharsheet("Ascii", "O")
	restore P: InsertIntoCharsheet("Ascii", "P")
	restore Q: InsertIntoCharsheet("Ascii", "Q")
	restore R: InsertIntoCharsheet("Ascii", "R")
	restore S: InsertIntoCharsheet("Ascii", "S")
	restore T: InsertIntoCharsheet("Ascii", "T")
	restore U: InsertIntoCharsheet("Ascii", "U")
	restore V: InsertIntoCharsheet("Ascii", "V")
	restore W: InsertIntoCharsheet("Ascii", "W")
	restore X: InsertIntoCharsheet("Ascii", "X")
	restore Y: InsertIntoCharsheet("Ascii", "Y")
	restore Z: InsertIntoCharsheet("Ascii", "Z")
	restore More: InsertIntoCharsheet("Ascii", "Ã†")
	Restore UA: InsertIntoCharsheet("Unicode", "A")
	restore UB: InsertIntoCharsheet("Unicode", "B")
	restore UC: InsertIntoCharsheet("Unicode", "C")
	restore UD: InsertIntoCharsheet("Unicode", "D")
	restore UE: InsertIntoCharsheet("Unicode", "E")
	restore UF: InsertIntoCharsheet("Unicode", "F")
	restore UG: InsertIntoCharsheet("Unicode", "G")
	restore UH: InsertIntoCharsheet("Unicode", "H")
	restore UI: InsertIntoCharsheet("Unicode", "I")
	restore UJ: InsertIntoCharsheet("Unicode", "J")
	restore UK: InsertIntoCharsheet("Unicode", "K")
	restore UL: InsertIntoCharsheet("Unicode", "L")
	restore UM: InsertIntoCharsheet("Unicode", "M")
	restore UN: InsertIntoCharsheet("Unicode", "N")
	restore UO: InsertIntoCharsheet("Unicode", "O")
	restore UP: InsertIntoCharsheet("Unicode", "P")
	restore UQ: InsertIntoCharsheet("Unicode", "Q")
	restore UR: InsertIntoCharsheet("Unicode", "R")
	restore US: InsertIntoCharsheet("Unicode", "S")
	restore UT: InsertIntoCharsheet("Unicode", "T")
	restore UU: InsertIntoCharsheet("Unicode", "U")
	restore UV: InsertIntoCharsheet("Unicode", "V")
	restore UW: InsertIntoCharsheet("Unicode", "W")
	restore UX: InsertIntoCharsheet("Unicode", "X")
	restore UY: InsertIntoCharsheet("Unicode", "Y")
	restore UZ: InsertIntoCharsheet("Unicode", "Z")
end sub

